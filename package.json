{
  "name": "mm-service-aws-lamda",
  "version": "1.0.0",
  "description": "API Implementation for MM",
  "main": "src/app.js",
  "scripts": {
    "_checkdeployenv": "node deploy/nodejs/validate-deploy-env.js",
    "_lambdazip": "node deploy/nodejs/aws-lambda-zip.js",
    "_yamlmerge": "node deploy/nodejs/swagger-yaml-merge",
    "_applyawsconfig": "node deploy/nodejs/aws-apply-config",
    "_applyperm": "chmod a+x -R ./deploy/scripts/aws/*.sh",
    "_md5": "node deploy/nodejs/hash-files.js",
    "_uploadartifacts": "./deploy/scripts/aws/archieve_n_upload.sh",
    "_deletestack": "./deploy/scripts/aws/delete_api_stack.sh",
    "_createorupdatestack": "./deploy/scripts/aws/create_or_update_api_stack.sh",
    "clean": "rimraf dist",
    "prepdeploy": "npm run _checkdeployenv && npm run clean && npm run _lambdazip && npm run _yamlmerge && npm run _applyawsconfig && npm run _applyperm",
    "deploy": "npm run _md5 && npm run _uploadartifacts && npm run _createorupdatestack",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/v8-suresh/mm-service-aws-lamda.git"
  },
  "keywords": [
    "aws",
    "lamda",
    "serverless",
    "api gateway",
    "rest",
    "api"
  ],
  "author": "suresh.sivanantham@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/v8-suresh/mm-service-aws-lamda/issues"
  },
  "homepage": "https://github.com/v8-suresh/mm-service-aws-lamda#readme",
  "dependencies": {
    "aws-serverless-express": "^1.1.3",
    "express": "^4.14.0",
    "glob": "^7.1.1",
    "lodash": "^4.16.4",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "adm-zip": "^0.4.7",
    "archiver": "^1.1.0",
    "json-refs": "^2.1.6",
    "md5-file": "^3.1.1",
    "rimraf": "^2.5.2",
    "yaml-js": "^0.1.4"
  },
  "config": {
    "AWS_S3_BUCKET": "mm-lamda-deployments",
    "AWS_CF_STACK_NAME": "",
    "AWS_REGION": ""
  }
}
