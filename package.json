{
  "name": "mm-service-aws-lamda",
  "version": "1.0.0",
  "description": "API Implementation for MM",
  "main": "src/app.js",
  "scripts": {
    "checkdeployenv": "node deploy/nodejs/validate-deploy-env.js",
    "clean": "rimraf dist",
    "lambdazip": "node deploy/nodejs/aws-lambda-zip.js",
    "yamlmerge": "node deploy/nodejs/swagger-yaml-merge",
    "applyawsconfig": "node deploy/nodejs/aws-apply-config",
    "applyperm": "chmod a+x -R ./deploy/scripts/aws/*.sh",
    "uploadartifacts": "./deploy/scripts/aws/push_deploy_art_to_s3.sh",
    "deletestack": "./deploy/scripts/aws/delete_api_stack.sh",
    "createorupdatestack": "./deploy/scripts/aws/create_or_update_api_stack.sh",
    "createstack": "./deploy/scripts/aws/create_api_stack.sh",
    "prepdeploy": "npm run checkdeployenv && npm run clean && npm run lambdazip && npm run yamlmerge && npm run applyawsconfig && npm run applyperm",
    "deploy": "npm run uploadartifacts && npm run createorupdatestack",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pwd": "pwd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/v8-suresh/mm-service-aws-lamda.git"
  },
  "keywords": [
    "aws",
    "lamda"
  ],
  "author": "suresh.sivanantham@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/v8-suresh/mm-service-aws-lamda/issues"
  },
  "homepage": "https://github.com/v8-suresh/mm-service-aws-lamda#readme",
  "dependencies": {
    "aws-serverless-express": "^1.1.3",
    "express": "^4.14.0",
    "glob": "^7.1.1",
    "lodash": "^4.16.4",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "adm-zip": "^0.4.7",
    "archiver": "^1.1.0",
    "json-refs": "^2.1.6",
    "rimraf": "^2.5.2",
    "yaml-js": "^0.1.4"
  },
  "config": {
    "AWS_S3_BUCKET": "mm-lamda-deployments",
    "AWS_CF_STACK_NAME": "",
    "AWS_REGION": ""
  }
}
